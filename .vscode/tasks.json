{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "no util task",
            "command": "gcc",
            "args": [
                "-g",
                "${file}",
                "-o",
                "a.out",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "with util task",
            "command": "gcc",
            "args": [
                "-g",
                "-o",
                "a.out",
                "${file}",
                "../util.c"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}
