{
    "code-runner.executorMapByFileExtension": {
        ".c": "cd $dir && cc $fileName && ./a.out"
    },
    "C_Cpp.default.compilerPath": "/usr/local/bin/gcc-11",
    "files.exclude": {
        "**/*.dSYM": true,
        "**/.DS_Store": true
    },
    "code-runner.executorMapByGlob": {
        "decrementing-order.c": "cd $dir && gcc -o a.out $fileName ../util.c && ./a.out",
        "selection-sort.c": "cd $dir && gcc -o a.out $fileName ../util.c && ./a.out",
        "merge-sort.c": "cd $dir && gcc -o a.out $fileName ../util.c && ./a.out",
        "*.c": "cd $dir && gcc -o a.out $fileName ../util.c && ./a.out"
    },
    "code-runner.runInTerminal": true,
    "files.associations": {
        "stdio.h": "c",
        "util.h": "c"
    },
}
